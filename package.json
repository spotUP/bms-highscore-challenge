{
  "name": "retroranks",
  "private": true,
  "version": "1.0.0",
  "description": "RetroRanks - The ultimate arcade gaming platform for tracking high scores, organizing tournaments, and celebrating champions",
  "homepage": "https://retroranks.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/spotUP/bms-highscore-challenge"
  },
  "keywords": [
    "arcade",
    "games",
    "highscore",
    "tournament",
    "retro",
    "gaming",
    "leaderboard",
    "competition"
  ],
  "author": "RetroRanks Team",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "predev": "tsx scripts/check-env.ts",
    "dev": "npm run websocket & npx tsx scripts/logo-proxy-server.ts & vite",
    "dev:full": "npm run websocket & vite",
    "prebuild": "tsx scripts/check-env.ts && tsx scripts/build-database-for-vercel.ts && mkdir -p public/sql.js && cp node_modules/sql.js/dist/sql-wasm.wasm public/sql.js/ && cp node_modules/sql.js/dist/sql-wasm-debug.wasm public/sql.js/",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "postbuild": "tsx scripts/deploy-tests.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . || true",
    "preview": "vite preview",
    "start": "tsx scripts/pong-websocket-server.ts",
    "websocket": "tsx --watch scripts/pong-websocket-server.ts",
    "dedupe-achievements": "tsx scripts/remove-duplicate-achievements.ts",
    "apply-migration": "tsx scripts/apply-migration.ts",
    "check-env": "tsx scripts/check-env.ts",
    "verify-storage": "tsx scripts/verify-storage.ts",
    "deploy-tests": "tsx scripts/deploy-tests.ts",
    "build-database": "tsx scripts/build-database-for-vercel.ts"
  },
